alias g=grep
alias c=cat

alias less='less -r'
alias tree='tree -AC'
alias json='python -m json.tool'

alias cc='pygmentize -f256 -Ostyle=vs'
function show() {
  cc $* | grep -n '' | less
}

alias scp='scp -C'

alias ..='cd ..'
alias cd..='cd ..'

alias br='git br'
alias st='git st'
alias f='git fetch'
alias l='git l'
