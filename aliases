alias g=grep
alias c=cat
alias m=make

alias e=npp

alias less='less -r'
alias tree='tree'
alias json='python -m json.tool'

alias cc='pygmentize -f256 -Ostyle=vs -g'
function show() {
  cc $* | grep -n '' | less
}

alias scp='scp -C'

alias ..='cd ..'
alias cd..='cd ..'

alias br='git br'
alias st='git st'
function f() { git fetch -q $* ; git br; }
alias l='git l'
alias blame='tig blame'

function highlight() {
  egrep --color "$*|"
}
alias h=highlight