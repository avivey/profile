[alias]
  st = status
  br = branch -v
  co = checkout
  ci = commit
  unstash = stash pop
  r = rebase master
  l = log --decorate --oneline --first-parent master~2..
  ll = log --decorate --oneline -3
  files = log --name-status --oneline --decorate master..


[color]
	ui = auto
[push]
        default = simple
